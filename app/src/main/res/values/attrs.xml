<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SubscribedContestsStyle">
        <attr name="heading" format="string" />
        <attr name="startDate" format="string" />
    </declare-styleable>
    <declare-styleable name="MediaStyle">
        <attr name="mediaTitle" format="string" />
        <attr name="username" format="string" />
        <attr name="views" format="integer" />
        <attr name="imageUrl" format="reference" />
    </declare-styleable>
    <declare-styleable name="ThreadStyle">
        <attr name="threadTitle" format="string" />
        <attr name="startedBy" format="string" />
        <attr name="replies" format="integer" />
    </declare-styleable>
    <declare-styleable name="AthleteCardStyle">
        <attr name="countryImage" format="reference" />
        <attr name="athleteName" format="string" />
    </declare-styleable>
    <declare-styleable name="ContestStyle">
        <attr name="contestName" format="string" />
        <attr name="contestDate" format="string" />
        <attr name="contestLocation" format="string" />
        <attr name="contestCategory" format="string" />
        <attr name="contestCompleted" format="boolean" />
        <attr name="contestWinner" format="string" />
        <attr name="contestWinnerCountryImage" format="reference" />
    </declare-styleable>
</resources>